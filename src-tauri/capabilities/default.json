{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "local": true,
  "remote": {
    "urls": ["http://localhost:*"]
  },
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-set-fullscreen",
    "core:window:allow-is-fullscreen",
    "opener:default"
  ]
}
